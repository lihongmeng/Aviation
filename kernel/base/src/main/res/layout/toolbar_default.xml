<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
  <com.hzlz.aviation.library.widget.widget.GVideoConstraintLayout
      android:id="@+id/root_layout"
      style="@style/Toolbar.Container"
      android:layout_height="wrap_content">
    <com.hzlz.aviation.library.widget.widget.GVideoImageView
        android:id="@+id/image_view_left_back"
        style="@style/Toolbar.LeftBack"
        android:padding="@dimen/DIMEN_4DP"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <com.hzlz.aviation.library.widget.widget.GVideoTextView
        android:id="@+id/text_view_title"
        style="@style/Toolbar.Title"
        android:paddingStart="@dimen/DIMEN_50DP"
        android:paddingEnd="@dimen/DIMEN_50DP"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <com.hzlz.aviation.library.widget.widget.GVideoTextView
        android:id="@+id/text_view_right_operation"
        style="@style/Toolbar.RightOperation"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <com.hzlz.aviation.library.widget.widget.GVideoImageView
        android:id="@+id/image_view_right_operation"
        style="@style/Toolbar.RightBack"
        android:padding="@dimen/DIMEN_5DP"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
  </com.hzlz.aviation.library.widget.widget.GVideoConstraintLayout>
</layout>
