<?xml version="1.0" encoding="utf-8"?>
<layout>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/root"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipChildren="true"
        android:background="@drawable/shape_soild_ffffff_coners_6dp_stroke_1dp">

        <com.hzlz.aviation.library.widget.widget.GVideoImageView
            android:id="@+id/icon"
            android:layout_width="@dimen/DIMEN_54DP"
            android:layout_height="@dimen/DIMEN_54DP"
            android:background="@drawable/icon_link_blue_left_corners"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.hzlz.aviation.library.widget.widget.GVideoImageView
            android:id="@+id/delete"
            android:layout_width="@dimen/DIMEN_54DP"
            android:layout_height="@dimen/DIMEN_54DP"
            android:src="@drawable/icon_clear"
            android:scaleType="center"
            android:visibility="gone"
            android:layout_alignParentRight="true"
            tools:visibility="visible"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

        <com.hzlz.aviation.library.widget.widget.GVideoTextView
            android:id="@+id/title"
            android:layout_width="0dp"
            android:layout_height="@dimen/DIMEN_54DP"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:maxLines="2"
            android:textColor="@color/color_666666"
            android:textSize="@dimen/sp_13"
            android:layout_marginStart="@dimen/DIMEN_10DP"
            android:layout_marginEnd="@dimen/DIMEN_10DP"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@id/icon"
            app:layout_constraintRight_toLeftOf="@id/delete"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</layout>