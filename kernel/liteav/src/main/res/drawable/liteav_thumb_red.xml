<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item>
    <shape android:shape="oval"
        android:useLevel="true">
      <size
          android:width="14dp"
          android:height="14dp" />
      <solid android:color="@color/color_000000_0" />
      <stroke
          android:color="#66FC284D"
          android:width="2dp"/>
    </shape>
  </item>

  <item android:bottom="2dp"
      android:top="2dp"
      android:left="2dp"
      android:right="2dp">
    <shape
        android:shape="oval"
        android:useLevel="true">
      <size
          android:width="14dp"
          android:height="14dp" />
      <solid android:color="#FFFFFF" />
    </shape>
  </item>
</layer-list>
