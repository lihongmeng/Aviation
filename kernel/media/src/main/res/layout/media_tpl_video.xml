<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <com.hzlz.aviation.library.widget.widget.GVideoLinearLayout
        android:id="@+id/video_layout"
        style="@style/feed_layout_margin_root"
        android:animateLayoutChanges="true">

        <include
            android:id="@+id/feed_toolbar_top"
            layout="@layout/media_toolbar_layout_top" />

        <include
            android:id="@+id/video_include"
            layout="@layout/media_video_template_layout" />

        <include
            android:id="@+id/feed_toolbar_include"
            layout="@layout/media_toolbar_layout_bottom" />

        <include
            android:id="@+id/feed_news_bottom"
            style="@style/feed_layout_margin_toolbar_bottom_news"
            layout="@layout/news_tpl_news_bottom" />

        <include
            android:id="@+id/feed_audiovisual_bottom"
            layout="@layout/media_toolbar_layout_bottom_audiovisual"/>

        <View
            android:id="@+id/line"
            style="@style/feed_layout_line" />

    </com.hzlz.aviation.library.widget.widget.GVideoLinearLayout>

</layout>