<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data></data>

    <com.hzlz.aviation.library.widget.widget.GVideoConstraintLayout
        android:id="@+id/root_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.hzlz.aviation.library.widget.widget.GVideoTextView
            android:id="@+id/content_tag"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/DIMEN_28DP"
            android:layout_marginStart="@dimen/feed_layout_margin"
            android:gravity="bottom"
            android:includeFontPadding="false"
            android:text="@string/tab_moment"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_13"
            android:visibility="gone"
            app:layout_constraintStart_toStartOf="parent"
            tools:visibility="gone" />

        <FrameLayout
            android:id="@+id/layout_content"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/content_tag" />

    </com.hzlz.aviation.library.widget.widget.GVideoConstraintLayout>
</layout>