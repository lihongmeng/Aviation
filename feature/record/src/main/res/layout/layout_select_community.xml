<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.hzlz.aviation.library.widget.widget.AviationTextView
        android:id="@+id/select_community_name"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:drawableStart="@drawable/home_tab_icon_community"
        android:gravity="center_vertical"
        android:singleLine="true"
        android:text="@string/select_community"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp_15"
        app:layout_constraintStart_toStartOf="parent"
        tools:text="1" />

    <com.hzlz.aviation.library.widget.widget.AviationTextView
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:drawableEnd="@drawable/icon_arrow_right_text"
        android:drawablePadding="@dimen/DIMEN_6DP"
        android:gravity="center_vertical|end"
        android:textColor="@color/color_7f7f7f"
        android:textSize="@dimen/sp_13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/select_community_name"
        tools:text="12345678912345768" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/color_f2f2f2"
        android:layout_marginStart="@dimen/DIMEN_14DP"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="@dimen/DIMEN_14DP" />

</merge>