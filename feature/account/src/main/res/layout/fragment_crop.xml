<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <data>
    <variable
        name="viewModel"
        type="com.hzlz.aviation.feature.account.ui.crop.CropViewModel"
        />
  </data>
  <com.hzlz.aviation.library.widget.widget.AviationFrameLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      >
    <com.hzlz.aviation.library.crop.CropImageView
        android:id="@+id/image_view_crop"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:cropShape="oval"
        app:cropGuidelines="off"
        app:cropAspectRatioX="1"
        app:cropAspectRatioY="1"
      />
  </com.hzlz.aviation.library.widget.widget.AviationFrameLayout>
</layout>
