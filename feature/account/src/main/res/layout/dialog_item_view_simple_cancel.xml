<?xml version="1.0" encoding="utf-8"?>
<layout>
  <data>
    <variable
        name="view"
        type="com.hzlz.aviation.feature.account.dialog.SimpleCancelDialogItemView"
        />
  </data>
  <com.hzlz.aviation.library.widget.widget.AviationTextView xmlns:android="http://schemas.android.com/apk/res/android"
      android:id="@+id/text_view_cancel"
      android:layout_width="match_parent"
      android:layout_height="@dimen/simple_cancel_item_view_height"
      android:background="@drawable/dialog_item_view_simple_cancel_background"
      android:gravity="center"
      android:onClick="@{()->view.cancel()}"
      android:orientation="vertical"
      android:text="@string/simple_cancel_item_view_cancel"
      android:textColor="@color/color_333333"
      />
</layout>
